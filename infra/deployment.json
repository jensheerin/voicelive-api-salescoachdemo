        },
        "PROJECT_ENDPOINT": {
            "type": "string",
            "value": "[format('{0}api/projects/default-project', reference('aiFoundryDeployment').outputs.endpoint.value)]"
        },
        "AZURE_OPENAI_ENDPOINT": {
            "type": "string",
            "value": "[reference('aiFoundryDeployment').outputs.endpoint.value]"
